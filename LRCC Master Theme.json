{
    "name": "Laurel Ridge Master Theme",
    "$schema": "https://raw.githubusercontent.com/microsoft/powerbi-desktop-samples/main/Report%20Theme%20JSON%20Schema/reportThemeSchema-2.145.json",
    "icons": [],
    "dataColors": [
      "#00426b",
      "#76bd22",
      "#8a85c9",
      "#ffd000",
      "#00a7cf",
      "#df1582"
    ],
    "tableAccent": "#00426b",
    "good": "#1AAB40",
    "neutral": "#D9B300",
    "bad": "#D64554",
    "maximum": "#00426b",
    "center": "#76bd22",
    "minimum": "#ffd000",
    "foreground": "#252423",
    "foregroundNeutralSecondary": "#605E5C",
    "backgroundLight": "#F3F2F1",
    "foregroundNeutralTertiary": "#B3B0AD",
    "background": "#FFFFFF",
    "backgroundNeutral": "#C8C6C4",
    "textClasses": {
      "callout": {
        "fontFace": "Arial",
        "color": "#252423",
        "fontSize": 45
      },
      "title": {
        "fontFace": "Arial",
        "color": "#252423",
        "fontSize": 12
      },
      "header": {
        "fontFace": "Arial",
        "color": "#252423",
        "fontSize": 12
      },
      "label": {
        "fontFace": "Arial",
        "color": "#252423",
        "fontSize": 10
      },
      "smallLightLabel": {
        "color": "#605E5C",
        "fontSize": 9
      },
      "largeLightLabel": {
        "color": "#605E5C",
        "fontSize": 12
      },
      "boldLabel": {
        "fontFace": "Arial"
      },
      "lightLabel": {
        "color": "#605E5C"
      },
      "smallLabel": {
        "fontSize": 9
      },
      "largeLabel": {
        "fontSize": 12
      },
      "semiboldLabel": {
        "fontFace": "Arial"
      },
      "largeTitle": {
        "fontSize": 14
      }
    },
    "visualStyles": {
      "*": {
        "*": {
          "background": [
            {
              "color": {
                "solid": {
                  "color": "#F3F2F1"
                }
              }
            }
          ],
          "outspace": [
            {
              "color": {
                "solid": {
                  "color": "#F3F2F1"
                }
              }
            }
          ],
          "title": [
            {
              "fontColor": {
                "solid": {
                  "color": {
                    "expr": {
                      "ThemeDataColor": {
                        "ColorId": 2,
                        "Percent": 0
                      }
                    }
                  }
                }
              },
              "alignment": "left",
              "fontFamily": "Arial",
              "bold": true,
              "italic": false,
              "underline": false
            }
          ]
        }
      }
    }
  }